<div class="quest_card pixel_card">
  <div>
    <p class="title">
      <%= quest_template.title %>
    </p>
    <p class="description">
      <%= quest_template.description %>
    </p>
    <div class="button_container flex_start">
      <%= button_to 'Edit', edit_quest_template_path(quest_template.id), method: :get, class: 'pixel_button' %>
      <%= button_to 'Delete', quest_template_path(quest_template.id), method: :delete, class: 'pixel_button' %>
    </div>
  </div>
</div>
